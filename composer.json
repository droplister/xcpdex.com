{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/droplister/xcp-core.git"
      }
    ],
    "require": {
        "php": "^7.2.5",
        "bitwasp/bitcoin": "v0.0.34.3",
        "curl/curl": "^2.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "droplister/xcp-core": "dev-feature/add-dispeners",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "irazasyed/telegram-bot-sdk": "dev-develop",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.4",
        "laravel/horizon": "^4.0",
        "laravel/tinker": "^2.0",
        "owenvoke/opensea": "^0.1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-responsecache": "^6.6",
        "vittominacori/coinmarketcap-php": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
